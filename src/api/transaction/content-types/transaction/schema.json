{
  "kind": "collectionType",
  "collectionName": "transactions",
  "info": {
    "singularName": "transaction",
    "pluralName": "transactions",
    "displayName": "transaction"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "book": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::book.book",
      "inversedBy": "transactions"
    },
    "borrower": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::borrower.borrower",
      "inversedBy": "transactions"
    },
    "transaction_date": {
      "type": "date",
      "required": true
    },
    "transaction_status": {
      "type": "enumeration",
      "enum": [
        "borrowed",
        "returned"
      ]
    }
  }
}
